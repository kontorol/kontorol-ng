{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2017",
      "dom"
    ],
    "paths": {
      "@kontorol-ng/kontorol-common": [
        "dist/kontorol-ng/kontorol-common"
      ],
      "@kontorol-ng/kontorol-common/*": [
        "dist/kontorol-ng/kontorol-common/*"
      ],
      "@kontorol-ng/kontorol-ui": [
        "dist/kontorol-ng/kontorol-ui"
      ],
      "@kontorol-ng/kontorol-ui/*": [
        "dist/kontorol-ng/kontorol-ui/*"
      ],
      "@kontorol-ng/kontorol-logger": [
        "dist/kontorol-ng/kontorol-logger"
      ],
      "@kontorol-ng/kontorol-logger/*": [
        "dist/kontorol-ng/kontorol-logger/*"
      ],
      "@kontorol-ng/kontorol-primeng-ui": [
        "dist/kontorol-ng/kontorol-primeng-ui"
      ],
      "@kontorol-ng/kontorol-primeng-ui/*": [
        "dist/kontorol-ng/kontorol-primeng-ui/*"
      ],
      "@kontorol-ng/mc-shared": [
        "dist/kontorol-ng/mc-shared"
      ],
      "@kontorol-ng/mc-shared/*": [
        "dist/kontorol-ng/mc-shared/*"
      ]
    }
  }
}